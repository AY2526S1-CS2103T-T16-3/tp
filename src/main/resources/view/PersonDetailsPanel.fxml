<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ScrollPane hbarPolicy="NEVER" vbarPolicy="AS_NEEDED" fitToWidth="true"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <content>
                <VBox spacing="8.0" styleClass="scroll-pane-content">
                    <Label fx:id="name" styleClass="cell_huge_label" />
                    <Label fx:id="type" styleClass="cell_bigger_label" />
                    <Label fx:id="tagsLine" styleClass="cell_bigger_label" wrapText="true"/>
                    <Label fx:id="phone" styleClass="cell_bigger_label" />
                    <Label fx:id="email" styleClass="cell_bigger_label" />
                    <Label fx:id="address" styleClass="cell_bigger_label" />
                    <Label fx:id="weddingDate" styleClass="cell_bigger_label" />
                    <Label fx:id="price" styleClass="cell_bigger_label" />
                    <Label fx:id="budget" styleClass="cell_bigger_label" />
                    <Label fx:id="linkedPersonsLine" styleClass="cell_bigger_label" wrapText="true" maxWidth="Infinity"/>
                </VBox>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
