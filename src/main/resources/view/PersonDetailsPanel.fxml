<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox spacing="8.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <Label fx:id="name" styleClass="cell_huge_label" />
            <Label fx:id="phone" styleClass="cell_bigger_label" />
            <Label fx:id="email" styleClass="cell_bigger_label" />
            <FlowPane fx:id="tags" hgap="6" vgap="6" />
        </VBox>
    </children>
</AnchorPane>
